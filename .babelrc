{
  "presets": [
    ["next/babel", {
      "preset-env": {
        "modules": "auto"
      },
      "transform-runtime": {
        "useESModules": false
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-function-bind",
    "lodash",
    ["react-intl", {
      "messagesDir": "./dist/messages/"
    }],
    "styled-components"
  ],
  "env": {
    "development" : {
      "compact": false
    }
  }
}
